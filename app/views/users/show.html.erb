<h1>Welcome</h1>
<%= current_user.email %>
<%= link_to "New Question", new_user_question_path(current_user) %>
<h2>Your questions</h2>
<% if current_user.questions.any? %>
  <ul>
    <% current_user.questions.each do |question| %>
      <li> <%= link_to question.title, user_question_path(current_user, question) %> </li>
    <% end %>
  </ul>
<% end %>
